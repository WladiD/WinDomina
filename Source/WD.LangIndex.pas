unit WD.LangIndex;

interface

const
  // Constants for Language Strings
  LS_0  = 0; // WinDomina
  LS_1  = 1; // Close
  LS_2  = 2; // Start dominate mode
  LS_3  = 3; // End dominate mode
  LS_4  = 4; // Double-tap the CapsLock key to activate/deactivate the dominate mode
  LS_5  = 5; // Dominate mode is active
  LS_6  = 6; // Dominate mode is not active
  LS_7  = 7; // Double-tap the CapsLock key...
  LS_8  = 8; // Clear
  LS_9  = 9; // Select window and center the cursor
  LS_10 = 10; // Move the current window to the next edge and align
  LS_11 = 11; // Stretch the current window to the next edge
  LS_12 = 12; // Shrink the current window to adjacent edge
  LS_13 = 13; // Restore previous position/size of the current window
  LS_14 = 14; // Place the current window in the cell
  LS_15 = 15; // Place the current window over multiple selected cells
  LS_16 = 16; // Key Assignment

implementation

end.
